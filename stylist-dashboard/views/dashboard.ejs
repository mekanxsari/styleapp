<!DOCTYPE html>
<html>
<head>
  <title>Clothes Dashboard</title>
</head>
<body>
  <h1>Clothes List</h1>
  <ul>
    <% clothes.forEach(item => { %>
      <li><%= item.name %></li>
    <% }) %>
  </ul>

  <h2>Add New Clothes</h2>
  <form method="POST" action="/add-clothes">
    <input type="text" name="name" placeholder="Clothes name" required />
    <button type="submit">Add</button>
  </form>
</body>
</html>
